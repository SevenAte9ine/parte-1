CREATE TABLE usuario (
NOME VARCHAR(30) PRIMARY KEY NOT NULL,
SENHA SERIAL NOT NULL);

CREATE TABLE livros (
ISBN SERIAL PRIMARY KEY NOT NULL,
NOME VARCHAR(30) NOT NULL,
AUTORES VARCHAR(60) NOT NULL,
EDITORA VARCHAR(30) NOT NULL,
ANO INT NOT NULL,
ALUGADO VARCHAR(30) NOT NULL,
ALUGADOR VARCHAR(30) NOT NULL);

CREATE TABLE autores (
NOME VARCHAR(30) PRIMARY KEY NOT NULL,
PAIS VARCHAR(30) NOT NULL);

CREATE TABLE clientes (
MATRICULA SERIAL PRIMARY KEY NOT NULL,
NOME VARCHAR(30)  NOT NULL,
TELEFONE SERIAL NOT NULL,
RETIRADAS INT NOT NULL);

select * from clientes

select * from autores

select * from livros

select * from usuario

drop table livros

drop table clientes

